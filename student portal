class Student:
    def _init_(self, name, roll_number, cgpa):
         self.name = name
         self.roll_number = roll_number
         self.cgpa = cgpa
class PlacementSystem:
     def _init_(self):
         self.students = []
     def add_student(self, student):
         self.students.append(student)
     def eligible_students(self, cgpa_threshold):
         return [student for student in self.students if student.cgpa >= cgpa_threshold]

student1 = Student("Anu", “22001”,8.0)
student2 = Student("John Doe", "22002", 7.5)
student3 = Student("Jane Smith", "22003", 9.2)
placement_system = PlacementSystem()
placement_system.add_student(student1)
placement_system.add_student(student2)
placement_system.add_student(student3)
eligible_students = placement_system.eligible_students(8.0)
print("Eligible Students:")
for student in eligible_students:
 print(f"Name: {student.name}, Roll Number: {student.roll_number}, CGPA: {student.cgpa}")
